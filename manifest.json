{
  "manifest_version": 3,
  "name": "Course Hierarchy Comparison",
  "version": "1.0.0",
  "description": "Compare course hierarchies (IC, CR, Honors) to ensure consistency and compliance",
  "permissions": [
    "sidePanel"
  ],
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "action": {
    "default_title": "Open Course Comparison"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}

